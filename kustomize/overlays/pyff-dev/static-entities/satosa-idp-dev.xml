<ns0:EntityDescriptor xmlns:ns0="urn:oasis:names:tc:SAML:2.0:metadata" xmlns:ns1="urn:mace:shibboleth:metadata:1.0" xmlns:ns2="urn:oasis:names:tc:SAML:metadata:ui" xmlns:ns3="http://www.w3.org/2000/09/xmldsig#" entityID="https://edugain-idp-dev.web.cern.ch/proxysamlfrontend">
    <ns0:IDPSSODescriptor protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol" WantAuthnRequestsSigned="false">
        <ns0:Extensions>
            <ns1:Scope regexp="false">edugain-idp-dev.cern.ch</ns1:Scope>
            <ns2:UIInfo>
                <ns2:DisplayName xml:lang="en">CERN Identity Provider</ns2:DisplayName>
                <ns2:Description xml:lang="en">CERN (European Organization for Nuclear Research)</ns2:Description>
                <ns2:Logo height="60" width="60">data:image/png;base64,</ns2:Logo>
                <ns2:InformationURL xml:lang="en">https://cern.ch</ns2:InformationURL>
                <ns2:PrivacyStatementURL xml:lang="en">https://home.cern/data-privacy-protection-statement</ns2:PrivacyStatementURL>
            </ns2:UIInfo>
        </ns0:Extensions>
        <ns0:KeyDescriptor use="signing">
            <ns3:KeyInfo>
                <ns3:X509Data>
                    <ns3:X509Certificate></ns3:X509Certificate>
                </ns3:X509Data>
            </ns3:KeyInfo>
        </ns0:KeyDescriptor>
        <ns0:KeyDescriptor use="encryption">
            <ns3:KeyInfo>
                <ns3:X509Data>
                    <ns3:X509Certificate></ns3:X509Certificate>
                </ns3:X509Data>
            </ns3:KeyInfo>
        </ns0:KeyDescriptor>
        <ns0:NameIDFormat>urn:oasis:names:tc:SAML:2.0:nameid-format:transient</ns0:NameIDFormat>
        <ns0:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect" Location="https://edugain-idp-dev.web.cern.ch/saml2sp/sso/redirect"/>
        <ns0:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST" Location="https://edugain-idp-dev.web.cern.ch/saml2sp/sso/post"/>
    </ns0:IDPSSODescriptor>
    <ns0:Organization>
        <ns0:OrganizationName xml:lang="en">cern.ch</ns0:OrganizationName>
        <ns0:OrganizationDisplayName xml:lang="en">CERN Identity Provider</ns0:OrganizationDisplayName>
        <ns0:OrganizationURL xml:lang="en">https://www.cern.ch</ns0:OrganizationURL>
    </ns0:Organization>
    <ns0:ContactPerson contactType="technical">
        <ns0:GivenName>CERN Service Desk</ns0:GivenName>
        <ns0:EmailAddress>mailto:service-desk@cern.ch</ns0:EmailAddress>
    </ns0:ContactPerson>
    <ns0:ContactPerson contactType="administrative">
        <ns0:GivenName>CERN Service Desk</ns0:GivenName>
        <ns0:EmailAddress>mailto:service-desk@cern.ch</ns0:EmailAddress>
    </ns0:ContactPerson>
    <ns0:ContactPerson contactType="support">
        <ns0:GivenName>CERN Service Desk</ns0:GivenName>
        <ns0:EmailAddress>mailto:service-desk@cern.ch</ns0:EmailAddress>
    </ns0:ContactPerson>
</ns0:EntityDescriptor>
